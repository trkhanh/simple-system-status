{
    "name": "system-status",
    "private": false,
    "version": "1.0.0",
    "description": "A simple system status page for management incident",
    "main": "index.js",
    "scripts": {
      "dev": "parcel src/index.html",
      "build": "npm run build:site",
      "fix": "prettier --print-width 120 --write 'src/**/*.{js,html,scss}'",
      "build:site": "parcel build src/index.html --no-minify --public-url /"
    },
    "repository": "system-status",
    "author": "trkhanh8@gmail.com>",
    "homepage": "https://github.com/trkhanh8/simple-system-status#readme",
    "devDependencies": {
      "autoprefixer": "^8.6.5",
      "node-sass": "^4.9.3",
      "parcel-bundler": "^1.10.3"
    },
    "dependencies": {
      "normalize.css": "^8.0.0",
      "timeago.js": "^4.0.0-beta.2"
    },
    "postcss": {
      "plugins": {
        "autoprefixer": true
      }
    },
    "license": "",
    "browserslist": [
      "last 2 Chrome versions",
      "last 2 Safari versions",
      "last 2 Firefox versions",
      "last 2 Edge versions"
    ]
  }